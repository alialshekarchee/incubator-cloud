    <script src="/socket.io/socket.io.js"></script>
    <script>

        var temp = document.getElementById('temp_text'),
            tempMin = document.getElementById('temp_text_min'),
            tempMax = document.getElementById('temp_text_max'),
            humidity = document.getElementById('humidity_text'),
            humidityMin = document.getElementById('humidity_text_min'),
            humidityMax = document.getElementById('humidity_text_max'),
            fan = document.getElementById('fanpath'),
            power1 = document.getElementById('power1'),
            power2 = document.getElementById('power2'),
            power3 = document.getElementById('power3'),
            door1 = document.getElementById('door1'),
            door2 = document.getElementById('door2'),
            lamp = document.getElementById('lamp'),
            water = document.getElementById('water'),
            egg = document.getElementById('egg');


        var socket = io();
        socket.on('connect', () => socket.emit('payload', { token: '<%= token %>', client: 'viewer', request: { destination: '<%= destination %>', msg: 'register' } }));

        socket.on('payload', payload => {
            // console.log(payload);
            // list.innerHTML += '<li>' + payload + '</li>'
            var payloadObj = JSON.parse(payload);
            if (payloadObj["CMD"] === "getParameters") {
                tempMin.innerText = payloadObj["TMPLO"] + "°";
                tempMax.innerText = payloadObj["TMPHI"] + "°";;
                humidityMin.innerText = payloadObj["HLO"] + "%";
                humidityMax.innerText = payloadObj["HHI"] + "%";
            }
            else if (payloadObj["CMD"] === "getData") {
                temp.innerText = Math.round(parseFloat(payloadObj["TEMP"]) * 10) / 10 + "°C";
                humidity.innerText = Math.round(parseFloat(payloadObj["HUMI"]) * 10) / 10 + "%";

                var a = payloadObj.GROP & 1, // turn eggs
                    b = payloadObj.GROP >> 1; b &= 1, // water
                        c = payloadObj.GROP >> 2; c &= 1, // lamp
                            d = payloadObj.GROP >> 3; d &= 1, // door
                                e = payloadObj.GROP >> 4; e &= 1, // battery
                                    f = payloadObj.GROP >> 5; f &= 1; // fan
                if (f !== 1) {
                    fan.style.fill = "#98c1d9";
                    fan.classList.add("rotating");
                } else {
                    fan.style.fill = "white";
                    fan.classList.remove("rotating");
                }
                if (e !== 1) {
                    power1.style.fill = "#ee6c4d";
                    power2.style.fill = "#ee6c4d";
                    power3.style.fill = "#ee6c4d";
                } else {
                    power1.style.fill = "white";
                    power2.style.fill = "white";
                    power3.style.fill = "white";
                }
                if (d == 1) {
                    door1.style.fill = "#ee6c4d";
                    door2.style.fill = "#ee6c4d";
                } else {
                    door1.style.fill = "white";
                    door2.style.fill = "white";
                }

                if (c !== 1) {
                    lamp.style.fill = "yellow";
                } else {
                    lamp.style.fill = "white";
                }
                if (b !== 1) {
                    water.style.fill = "#ee6c4d";
                } else {
                    water.style.fill = "white";
                }
                if (a == 1) {
                    egg.style.fill = "#98c1d9";
                    egg.classList.add("flipping");
                } else {
                    egg.style.fill = "white";
                    egg.classList.remove("flipping");
                }
            }
        });

    </script>
    <script type="text/javascript">
        function autorun() {
            socket.emit('payload', { token: '<%= token %>', client: 'viewer', request: { destination: '<%= destination %>', msg: "{'CMD':'getParameters','DATASET':''}" } });
            socket.emit('payload', { token: '<%= token %>', client: 'viewer', request: { destination: '<%= destination %>', msg: "{'CMD':'getData','DATASET':''}" } });
        }
        if (document.addEventListener) document.addEventListener("DOMContentLoaded", autorun, false);
        else if (document.attachEvent) document.attachEvent("onreadystatechange", autorun);
        else window.onload = autorun;
    </script>
